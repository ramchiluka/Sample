<h1>Editing Register</h1>
<%= form_for @register, :html => { :multipart => true } do |f| %>


<%= render(:partial => 'form', :locals => { :f => f}) %>
 <%= image_tag @register.image.url(:thumb) %>

<div class="field">
    <%= f.label :image %>
    <%= f.file_field :image %>
  </div>

 <div class="actions">
    <%= f.submit("Update Register") %>
 </div> 
<% end %>
<%= link_to 'Show', @register %> |
<%= link_to 'Back', registers_path %>
